@{ 
    Layout = "/Views/Shared/_Layout.cshtml";
}
<div class="marginTop">
    <h2 class="display-4">Manage your account</h2>

    <div>
        <h4>Change your account settings</h4>
        <hr />
        <div class="row">
            <div class="col-3 divided">
                @await Html.PartialAsync("_ManageNav")
            </div>
            <div class="col-8">
                @RenderBody()
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
        
